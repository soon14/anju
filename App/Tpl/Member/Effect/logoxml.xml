    <image>
        <left url="{:CC('web_root')}{$viewrow['left']}" />
        <front url="{:CC('web_root')}{$viewrow['front']}" />
        <right url="{:CC('web_root')}{$viewrow['right']}" />
        <back url="{:CC('web_root')}{$viewrow['back']}" />
        <up url="{:CC('web_root')}{$viewrow['up']}" />
        <down url="{:CC('web_root')}{$viewrow['down']}" />
