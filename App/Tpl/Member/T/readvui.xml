<?xml version="1.0" encoding="UTF-8"?>
<krpano>
<volist name="row" id="ui">
    <switch name="ui['uitype']" >
        <case value="superimage">
            <plugin devices="flash" name="ui{$ui.id}" keep="False" url="{:CC('web_root')}{$ui.url}" align="{$ui.align}" edge="{$ui.edge}" parent="{$ui.parent_name}"
            x="{$ui.x}" y="{$ui.y}" scale="{$ui.scale}" alpha="{$ui.alpha}" rotate="{$ui.rotate}" zorder="{$ui.zorder}" visible="true"
            crop="{$ui.crop}" onovercrop="{$ui.hovercrop}" ondowncrop="{$ui.downcrop}"
            {$ui.action}
            />
        </case>
        <case value="video">
            <plugin devices="flash" name="ui_{$ui.id}" url="%SWFPATH%/plugins/videoplayer.swf" alturl="%SWFPATH%/plugins/videoplayer.js"
                    updateeveryframe="true" videourl="{:CC('web_root')}{$ui.url}" loop="True" align="{$ui.align}" edge="{$ui.edge}"
                    x="{$ui.x}" y="{$ui.y}" ox="0" oy="0" parent="ui_{$ui.parent}"
                    volume="0.7" zorder="{$ui.zorder}" alpha="{$ui.alpha}"
                    scale="{$ui.scale}" rotate="{$ui.rotate}" visible="True" enabled="True" capture="True" keep="False"
                    {$ui.action}
                    />
        </case>
        <default />
        <plugin devices="flash" name="ui{$ui.id}" keep="False" url="{$ui.url}" align="{$ui.align}" edge="{$ui.edge}" parent="{$ui.parent_name}"
            x="{$ui.x}" y="{$ui.y}" scale="{$ui.scale}" alpha="{$ui.alpha}" rotate="{$ui.rotate}" zorder="{$ui.zorder}" visible="true"
            {$ui.action}
            />
    </switch>
</volist>


